{
  "$defs": {
    "AILintingConfig": {
      "description": "AI Linting Fixer specific configuration.",
      "properties": {
        "enabled": {
          "default": true,
          "env": "AI_LINTING_ENABLED",
          "title": "Enabled",
          "type": "boolean"
        },
        "default_provider": {
          "default": "openai",
          "env": "AI_LINTING_DEFAULT_PROVIDER",
          "title": "Default Provider",
          "type": "string"
        },
        "default_model": {
          "default": "gpt-4",
          "env": "AI_LINTING_DEFAULT_MODEL",
          "title": "Default Model",
          "type": "string"
        },
        "max_workers": {
          "default": 4,
          "env": "AI_LINTING_MAX_WORKERS",
          "title": "Max Workers",
          "type": "integer"
        },
        "max_fixes_per_run": {
          "default": 10,
          "env": "AI_LINTING_MAX_FIXES",
          "title": "Max Fixes Per Run",
          "type": "integer"
        },
        "timeout_seconds": {
          "default": 300,
          "env": "AI_LINTING_TIMEOUT",
          "title": "Timeout Seconds",
          "type": "integer"
        },
        "default_fix_types": {
          "env": "AI_LINTING_DEFAULT_FIX_TYPES",
          "items": {
            "type": "string"
          },
          "title": "Default Fix Types",
          "type": "array"
        },
        "confidence_threshold": {
          "default": 0.7,
          "env": "AI_LINTING_CONFIDENCE_THRESHOLD",
          "title": "Confidence Threshold",
          "type": "number"
        },
        "syntax_validation": {
          "default": true,
          "env": "AI_LINTING_SYNTAX_VALIDATION",
          "title": "Syntax Validation",
          "type": "boolean"
        },
        "create_backups": {
          "default": true,
          "env": "AI_LINTING_CREATE_BACKUPS",
          "title": "Create Backups",
          "type": "boolean"
        },
        "error_handler": {
          "$ref": "#/$defs/ErrorHandlerConfig"
        },
        "enable_metrics": {
          "default": true,
          "env": "AI_LINTING_ENABLE_METRICS",
          "title": "Enable Metrics",
          "type": "boolean"
        },
        "enable_database_logging": {
          "default": true,
          "env": "AI_LINTING_DATABASE_LOGGING",
          "title": "Enable Database Logging",
          "type": "boolean"
        },
        "enable_orchestration": {
          "default": false,
          "env": "AI_LINTING_ENABLE_ORCHESTRATION",
          "title": "Enable Orchestration",
          "type": "boolean"
        }
      },
      "title": "AILintingConfig",
      "type": "object"
    },
    "DatabaseConfig": {
      "description": "Database configuration.",
      "properties": {
        "url": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "env": "DATABASE_URL",
          "title": "Url"
        },
        "pool_size": {
          "default": 10,
          "env": "DATABASE_POOL_SIZE",
          "title": "Pool Size",
          "type": "integer"
        },
        "max_overflow": {
          "default": 20,
          "env": "DATABASE_MAX_OVERFLOW",
          "title": "Max Overflow",
          "type": "integer"
        },
        "pool_timeout": {
          "default": 30,
          "env": "DATABASE_POOL_TIMEOUT",
          "title": "Pool Timeout",
          "type": "integer"
        },
        "pool_recycle": {
          "default": 3600,
          "env": "DATABASE_POOL_RECYCLE",
          "title": "Pool Recycle",
          "type": "integer"
        },
        "echo": {
          "default": false,
          "env": "DATABASE_ECHO",
          "title": "Echo",
          "type": "boolean"
        }
      },
      "title": "DatabaseConfig",
      "type": "object"
    },
    "Environment": {
      "description": "Environment types.",
      "enum": [
        "development",
        "testing",
        "staging",
        "production"
      ],
      "title": "Environment",
      "type": "string"
    },
    "ErrorHandlerConfig": {
      "description": "Error handling configuration for AI linting fixer and other components.",
      "properties": {
        "enabled": {
          "default": true,
          "env": "ERROR_HANDLER_ENABLED",
          "title": "Enabled",
          "type": "boolean"
        },
        "log_errors": {
          "default": true,
          "env": "ERROR_HANDLER_LOG_ERRORS",
          "title": "Log Errors",
          "type": "boolean"
        },
        "display_errors": {
          "default": true,
          "env": "ERROR_HANDLER_DISPLAY_ERRORS",
          "title": "Display Errors",
          "type": "boolean"
        },
        "export_errors": {
          "default": false,
          "env": "ERROR_HANDLER_EXPORT_ERRORS",
          "title": "Export Errors",
          "type": "boolean"
        },
        "auto_categorize": {
          "default": true,
          "env": "ERROR_HANDLER_AUTO_CATEGORIZE",
          "title": "Auto Categorize",
          "type": "boolean"
        },
        "severity_threshold": {
          "default": "LOW",
          "env": "ERROR_HANDLER_SEVERITY_THRESHOLD",
          "title": "Severity Threshold",
          "type": "string"
        },
        "enable_recovery": {
          "default": true,
          "env": "ERROR_HANDLER_ENABLE_RECOVERY",
          "title": "Enable Recovery",
          "type": "boolean"
        },
        "max_retry_attempts": {
          "default": 3,
          "env": "ERROR_HANDLER_MAX_RETRIES",
          "title": "Max Retry Attempts",
          "type": "integer"
        },
        "retry_delay_seconds": {
          "default": 1.0,
          "env": "ERROR_HANDLER_RETRY_DELAY",
          "title": "Retry Delay Seconds",
          "type": "number"
        },
        "exponential_backoff": {
          "default": true,
          "env": "ERROR_HANDLER_EXPONENTIAL_BACKOFF",
          "title": "Exponential Backoff",
          "type": "boolean"
        },
        "use_colors": {
          "default": true,
          "env": "ERROR_HANDLER_USE_COLORS",
          "title": "Use Colors",
          "type": "boolean"
        },
        "use_emojis": {
          "default": true,
          "env": "ERROR_HANDLER_USE_EMOJIS",
          "title": "Use Emojis",
          "type": "boolean"
        },
        "verbose_mode": {
          "default": false,
          "env": "ERROR_HANDLER_VERBOSE",
          "title": "Verbose Mode",
          "type": "boolean"
        },
        "export_format": {
          "default": "json",
          "env": "ERROR_HANDLER_EXPORT_FORMAT",
          "title": "Export Format",
          "type": "string"
        },
        "export_directory": {
          "default": "./logs",
          "env": "ERROR_HANDLER_EXPORT_DIR",
          "title": "Export Directory",
          "type": "string"
        },
        "auto_export": {
          "default": false,
          "env": "ERROR_HANDLER_AUTO_EXPORT",
          "title": "Auto Export",
          "type": "boolean"
        },
        "enable_callbacks": {
          "default": true,
          "env": "ERROR_HANDLER_ENABLE_CALLBACKS",
          "title": "Enable Callbacks",
          "type": "boolean"
        },
        "external_notifications": {
          "default": false,
          "env": "ERROR_HANDLER_EXTERNAL_NOTIFICATIONS",
          "title": "External Notifications",
          "type": "boolean"
        },
        "integrate_with_logging": {
          "default": true,
          "env": "ERROR_HANDLER_INTEGRATE_LOGGING",
          "title": "Integrate With Logging",
          "type": "boolean"
        },
        "integrate_with_metrics": {
          "default": true,
          "env": "ERROR_HANDLER_INTEGRATE_METRICS",
          "title": "Integrate With Metrics",
          "type": "boolean"
        },
        "integrate_with_workflows": {
          "default": true,
          "env": "ERROR_HANDLER_INTEGRATE_WORKFLOWS",
          "title": "Integrate With Workflows",
          "type": "boolean"
        }
      },
      "title": "ErrorHandlerConfig",
      "type": "object"
    },
    "GitHubConfig": {
      "description": "GitHub integration configuration.",
      "properties": {
        "token": {
          "anyOf": [
            {
              "format": "password",
              "type": "string",
              "writeOnly": true
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "env": "GITHUB_TOKEN",
          "title": "Token"
        },
        "app_id": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "env": "GITHUB_APP_ID",
          "title": "App Id"
        },
        "private_key": {
          "anyOf": [
            {
              "format": "password",
              "type": "string",
              "writeOnly": true
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "env": "GITHUB_PRIVATE_KEY",
          "title": "Private Key"
        },
        "timeout": {
          "default": 30,
          "title": "Timeout",
          "type": "integer"
        },
        "base_url": {
          "default": "https://api.github.com",
          "title": "Base Url",
          "type": "string"
        }
      },
      "title": "GitHubConfig",
      "type": "object"
    },
